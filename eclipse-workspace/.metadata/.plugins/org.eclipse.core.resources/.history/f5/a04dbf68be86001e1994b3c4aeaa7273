package demo;

import java.util.Scanner;
import java.util.Arrays;

public class Test {
    public static void main(String[] args) {
        
    }
    
    class Circle{
    	double radius = 1;
    	Circle(){
    	}
    	Circle(double newRadius){
    		radius = newRadius;
    	}
    	
    	double getArea() {
    		return radius * radius * Math.PI;
    	}
    	
    	double getPerimeter() {
    		return 2 * Math.PI * radius;
    	}
    	
    	void setRadius (double newRadius) {
    		radius = newRadius;
    	}
    }
}

